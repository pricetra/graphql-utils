{
  "name": "graphql-utils",
  "version": "1.0.9",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types/*": {
      "import": "./dist/types/*",
      "require": "./dist/types/*"
    },
    "./documents/*": {
      "import": "./dist/documents/*",
      "require": "./dist/documents/*"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "gql-codegen": "graphql-codegen --config codegen.ts",
    "start": "npm run gql-codegen && npm run build",
    "bump-version": "node bumpVersion.js",
    "bump-version:minor": "node bumpVersion.js minor",
    "bump-version:major": "node bumpVersion.js major",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "@apollo/client": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.12.0",
    "graphql": "^16.8.1",
    "typescript": "^5.0.0",
    "tsup": "^8.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pricetra/graphql-utils.git"
  },
  "author": "Ayaan Siddiqui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pricetra/graphql-utils/issues"
  },
  "homepage": "https://github.com/pricetra/graphql-utils#readme"
}
