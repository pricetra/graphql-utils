{
  "name": "graphql-utils",
  "version": "1.0.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./documents/queries": {
      "import": "./dist/documents/queries/index.js",
      "types": "./dist/documents/queries/index.d.ts"
    },
    "./documents/mutations": {
      "import": "./dist/documents/mutations/index.js",
      "types": "./dist/documents/mutations/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "gql-codegen": "graphql-codegen --config codegen.ts",
    "start": "npm run gql-codegen && npm run build",
    "bump-version": "node bumpVersion.js",
    "bump-version:minor": "node bumpVersion.js minor",
    "bump-version:major": "node bumpVersion.js major",
    "prepublish": "npm run build",
    "release:patch": "npm run gql-codegen && npm run build && npm run bump-version:patch && npm publish",
    "release:minor": "npm run gql-codegen && npm run build && npm run bump-version:minor && npm publish",
    "release:major": "npm run gql-codegen && npm run build && npm run bump-version:major && npm publish"
  },
  "peerDependencies": {
    "@apollo/client": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.12.0",
    "graphql": "^16.8.1",
    "typescript": "^5.0.0",
    "tsup": "^8.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pricetra/graphql-utils.git"
  },
  "author": "Ayaan Siddiqui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pricetra/graphql-utils/issues"
  },
  "homepage": "https://github.com/pricetra/graphql-utils#readme"
}
